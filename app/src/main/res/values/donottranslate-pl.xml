    <!-- map image selection -->
    <string name="pl_select_image_help">
        Wybierz plik zawierający obraz mapy.\n\nUżywaj tylko obrazów, do których masz uprawnienia.\n
        \nMenu zawiera odnośniki do folderów z twoimi zdjęciami i plikami pobranymi z internetu.
    </string>
    <string name="pl_select_image_invalid">
        %s nie jest poprawnym plikiem obrazu.\nMusisz wybrać plik obrazu
    </string>
    <string name="pl_select_image_select_file">Wybierz plik z obrazem mapy</string>
    <string name="pl_select_image_processing">Przetwarzanie wybranego obrazu.\nProszę czekać…</string>

    <!-- map image point selection -->
    <string name="pl_image_point_help">
        Wybierz punkty, aby ustalić położenie na mapie Google.\n\nAby uzyskać lepsze rezultaty, wybierz
        odległe punkty.\n\nUnikaj wybierania punktów pomiędzy już wybranymi punktami.
    </string>
    <string name="pl_image_point_no_image">Nie wybrano obrazu mapy. Nie można wybrać punktów.</string>
    <string name="pl_image_point_select_for_matching">Wybierz punkt, aby dopasować mapę</string>

    <!-- map geo point selection -->
    <string name="pl_geo_point_help">
        Zmieniaj rozmiar i obracaj mapę Google, aby dopasować obraz mapy.\n\nUżyj suwaków do obracania
        i skalowania obrazu mapy oraz ustawienia przezroczystości.\n\nPrzycisk mapy przełącza widok mapy
        lub satelity.
    </string>
    <string name="pl_geo_point_mapview_failure">
        Błąd w utworzeniu widoku mapy.\nUtworzenie mapy nie jest możliwe.
    </string>
    <string name="pl_geo_point_rotate">Obrót</string>
    <string name="pl_geo_point_scale">Skalowanie</string>
    <string name="pl_geo_point_transparency">Przezroczystość</string>
    <string name="pl_map_mode">Tryb mapy</string>

    <!-- map editor -->
    <string name="pl_editor_help">
        Nazwij mapę i dodaj jej opis na liście map.\n\nDodaj 2-4 punkty, aby dowiązać i skalować obraz mapy.\n\n
        Dopasuj albo usuń punkty, przytrzymując długo daną pozycję.\n\nMapa może być zapisana po wykonaniu podglądu.
    </string>
    <string name="pl_editor_image_load_failed">Błąd w ładowaniu obrazu mapy</string>
    <string name="pl_editor_need_two_points">Przed wykonaniem podglądu musisz dodać co namniej 2 punkty</string>
