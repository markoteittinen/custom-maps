    <string name="hr_editor_tiepoints">Vezne točke</string>

    <!-- map preview -->
    <string name="hr_preview_help">
        Provjeri poravnanje slike karte pomoću zumiranja i pomicanja slike.\n\nCrna tipka
        dozvoljava podešavanje ili dodavanje veznih točaka.\n\nNAPOMENA: Neke slike ne mogu biti
        savršeno podešene.
    </string>
    <string name="hr_preview_mapview_failure">
        Neuspjelo kreiranje prikaza karte.\nPregled slike nije dostupan.
    </string>
