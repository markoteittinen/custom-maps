    <!-- map image selection -->
    <string name="hr_select_image_help">
        Odaberi datoteku koja sadrži sliku karte.\n\nKoristi samo one slike za koje imaš dozvolu.
        \n\nIzbornik sadrži linkove na slike s tvojim fotografijama i slikama preuzetih s interneta
    </string>
    <string name="hr_select_image_invalid">
        %s nije valjana slikovna datoteka.\nMoraš odabrati datoteku slike.
    </string>
    <string name="hr_select_image_select_file">Odaberi datoteku sa slikom karte</string>
    <string name="hr_select_image_processing">Obrađujem odabranu sliku\n.Molim pričekaj…</string>

    <!-- map image point selection -->
    <string name="hr_image_point_help">
        Odaberi točku za lociranje na Google karti.\n\nZa najbolje rezultate odaberi točke udaljene
        jedna od druge.\n\nIzbjegavaj odabirati točke između prethodno izabranih točaka.
    </string>
    <string name="hr_image_point_no_image">
        Slika karte nije odabrana. Ne mogu se odabirati točke.
    </string>
    <string name="hr_image_point_select_for_matching">Odaberi točku za usklađivanje.</string>

    <!-- map geo point selection -->
    <string name="hr_geo_point_help">
        Zumiraj ili pomakni Google kartu da se poklopi sa središtem thumbnaila\n\n Koristi klizače
        za rotiranje i mijenjanje veličine thumbnaila, te za prilagodbu prozirnosti.\n\nTipka karte
        izmjenjuje prikaz karte sa satelitskim prikazom.
    </string>
    <string name="hr_geo_point_mapview_failure">
        Neuspjelo kreiranje prikaza karte.\nKreiranje nije moguće.
    </string>
